{
  "name": "face-detection",
  "version": "0.1.0",
  "description": "Desktop application face detection on web-cam flow",
  "main": "main.js",
  "scripts": {
    "start": "electron . --asar=true",
    "pack": "electron-builder --dir",
    "rebuild": "electron-rebuild -f -w opencv4nodejs",
    "dev": "electron . --debug"
  },
  "keywords": [
    "electron",
    "opencv",
    "face-detection"
  ],
  "author": "Saimon Shaplygin",
  "license": "MIT",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "^1.7.3",
    "opencv": "6.1.0"
  },
  "dependencies": {
    "node-gyp": "^3.6.2"
  }
}
